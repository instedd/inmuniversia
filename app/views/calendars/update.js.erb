<% if @form.valid? %>
  $("#calendar-<%= @form.child_id %>").replaceWith('<%= j(render_calendar(@child)) %>').setupComponents();
<% else %>
  $("#calendar-<%= @form.child_id %> .calendar-edit").replaceWith('<%= j(render('dashboard/calendar_form', calendar_form: @form)) %>').setupComponents();
<% end %>